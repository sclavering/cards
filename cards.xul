    </hbox><spacer class="row-spacer"/>
    <hbox id="regiment-reserve"><spacer flex="1"/>
      <stack id="regiment-reserve-0" class="card-pile"/><spacer flex="1"/>
      <stack id="regiment-reserve-1" class="card-pile"/><spacer flex="1"/>
      <stack id="regiment-reserve-2" class="card-pile"/><spacer flex="1"/>
      <stack id="regiment-reserve-3" class="card-pile"/><spacer flex="1"/>
      <stack id="regiment-reserve-4" class="card-pile"/><spacer flex="1"/>
