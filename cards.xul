    <hbox id="regiment-foundations"><spacer flex="1"/>
      <!-- first 4 foundations are built up from Ace, last 4 down from King -->
      <stack id="regiment-foundation-0" class="card-pile"/><spacer flex="1"/>
      <stack id="regiment-foundation-1" class="card-pile"/><spacer flex="1"/>
      <stack id="regiment-foundation-2" class="card-pile"/><spacer flex="1"/>
      <stack id="regiment-foundation-3" class="card-pile"/><spacer flex="1"/>
      <stack id="regiment-foundation-4" class="card-pile"/><spacer flex="1"/>
      <stack id="regiment-foundation-5" class="card-pile"/><spacer flex="1"/>
