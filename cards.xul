      <stack id="acesup-pile-0" class="card-fan-down"/><spacer flex="1"/>
      <stack id="acesup-pile-1" class="card-fan-down"/><spacer flex="1"/>
      <stack id="acesup-pile-2" class="card-fan-down"/><spacer flex="1"/>
      <stack id="acesup-pile-3" class="card-fan-down"/><spacer flex="2"/>
      <stack id="acesup-foundation" class="card-pile"/><spacer flex="1"/>
    </hbox>
  </vbox>

