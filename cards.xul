      <stack id="regiment-tableau-1-4" class="card-pile"/><spacer flex="1"/>
      <stack id="regiment-tableau-1-5" class="card-pile"/><spacer flex="1"/>
      <stack id="regiment-tableau-1-6" class="card-pile"/><spacer flex="1"/>
      <stack id="regiment-tableau-1-7" class="card-pile"/><spacer flex="1"/>
    </hbox><spacer flex="1"/>
  </vbox>

  <!-- Simple Simon -->
  <vbox id="SimpleSimon" name="&simon.name;" menukey="&simon.menukey;"
      hidden="true" difficultyLevels="&difficulty:standard-levels;">
    <spacer class="row-spacer"/>
    <hbox>
      <spacer flex="1"/>
