<?xml version="1.0"?>

<?xml-stylesheet href="chrome://cards/content/skin/cards.css" type="text/css"?>

<!DOCTYPE window [
<!ENTITY % cardsDTD SYSTEM "chrome://cards/locale/cards.dtd">
%cardsDTD;

<!-- used for the difficultyLevels attribute on games' main element -->
<!ENTITY difficulty:standard-levels "&difficulty:easy-1suit;|&difficulty:medium-2suits;|&difficulty:hard-4suits;">
<!ENTITY difficulty:easy-and-hard   "&difficulty:easy-2suits;|&difficulty:hard-4suits;">
<!ENTITY difficulty:golf "&difficulty:easy-golf;|&difficulty:medium-1deck;|&difficulty:hard-2decks;">
]>

<window xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
  windowtype="cardgames-main"
  title  ="&cards-window-title;"
  id     ="cards-window"
  persist="screenX screenY width height sizemode"

  gameWonTitle="&game-won:title;"
  gameWonMessage="&game-won:message;"
  >
  <!-- gameWon attrs read and passed to nsIPromptService call.  Should be a stringbundle really -->

<!-- sets some var's needed for positioning of cards in javascript -->
<script type="application/x-javascript" src="skin/skin.js"/>

<script type="application/x-javascript" src="cardslib.js"/>
<script type="application/x-javascript" src="cardgame.js"/>
<script type="application/x-javascript" src="freecellmover.js"/>

<script type="application/x-javascript" src="acesup.js"/>
<script type="application/x-javascript" src="canfield.js"/>
<script type="application/x-javascript" src="doublesol.js"/>
<script type="application/x-javascript" src="freecell.js"/>
<script type="application/x-javascript" src="golf.js"/>
<script type="application/x-javascript" src="grandfather.js"/>
<script type="application/x-javascript" src="gypsy.js"/>
<script type="application/x-javascript" src="klondike.js"/>
<script type="application/x-javascript" src="regiment.js"/>
<script type="application/x-javascript" src="simon.js"/>
<script type="application/x-javascript" src="spider.js"/>
<script type="application/x-javascript" src="mod3.js"/>
<script type="application/x-javascript" src="yukon.js"/>

<commandset id="cards-commands">
  <command id="cmd:newgame" oncommand="Game.newGame();"/>
  <command id="cmd:hint"    oncommand="Game.hint();"/>
  <command id="cmd:undo"    oncommand="Game.undo();"/>
  <command id="cmd:restart" oncommand="Game.restart();"/>
  <command id="cmd:options" oncommand="Cards.showOptionsDialog();"/>
  <command id="cmd:setdifficulty" oncommand="Game.setDifficultyLevel(event.target.value);"/>
</commandset>

<keyset id="cards-keys">
  <key id="key:newgame" key="&key:newgame;" modifiers="accel" observes="command:newgame"/>
  <key id="key:restart" key="&key:restart;" modifiers="accel" observes="command:restart"/>
  <key id="key:undo"    key="&key:undo;"    modifiers="accel" observes="command:undo"/>
  <key id="key:hint"    key="&key:hint;"    modifiers="accel" observes="command:hint"/>
</keyset>


<!-- display -->
<toolbar id="cards-toolbar">
  <toolbarbutton id="game-type-menu" type="menu" label="&gamesMenu.label;">
    <!-- popup contents generated by js -->
    <menupopup id="menupopup-gametypes" oncommand="Cards.playGame(event.target.value);"/>
  </toolbarbutton>
  <!-- content of menu generated dynamically based on a game's difficultyLevels attribute -->
  <toolbarbutton id="game-difficulty-menu" type="menu" disabled="true" command="cmd:setdifficulty"
      label="&m-game-difficulty:label;" tooltiptext="&m-game-difficulty:tooltip;">
    <menupopup id="game-difficulty-popup"/>
  </toolbarbutton>
  <!-- toolbarbutton command="command:options" label="&options:label;" tooltiptext="&options:tooltip;"/ -->
  <toolbarbutton command="cmd:newgame" label="&newgame:label;" tooltiptext="&newgame:tooltip;"/>
  <toolbarbutton command="cmd:restart" label="&restart:label;" tooltiptext="&restart:tooltip;"/>
  <toolbarbutton command="cmd:undo"    label="&undo:label;"    tooltiptext="&undo:tooltip;"/>
  <toolbarbutton command="cmd:hint"    label="&hint:label;"    tooltiptext="&hint:tooltip;"/>
  <spacer flex="1"/>
  <hbox id="score-panel">
    <description value="&statusbar-score-label;"/>
    <description id="score-display"/>
  </hbox>
</toolbar>

<!--
spacing at top and between rows of cards is done with <spacer class="row-spacer"/>s
rather than CSS, because margins and padding are not included in the result of
boxEl.boxObject.top/left etc, so there is a jump (of the num of px of padding)
when starting to move a card, which looks abysmal

Games must all have a name and menukey attribute on their main element, and the id
must be the key used for that game in the Games hashtable.  the two attributes are
used when building the game types menu
-->

<stack id="current-game-display" flex="1">
  <!-- Aces Up -->
  <vbox id="AcesUp" name="&acesup.name;" menukey="&acesup.menukey;"
      hidden="true">
    <spacer class="row-spacer"/>
    <hbox>
      <spacer flex="1"/>
      <stack id="acesup-stock" class="card-pile"/><spacer flex="2"/>
      <stack id="acesup-pile-0" class="card-fan-down"/><spacer flex="1"/>
      <stack id="acesup-pile-1" class="card-fan-down"/><spacer flex="1"/>
      <stack id="acesup-pile-2" class="card-fan-down"/><spacer flex="1"/>
      <stack id="acesup-pile-3" class="card-fan-down"/><spacer flex="2"/>
      <stack id="acesup-foundation" class="card-pile"/><spacer flex="1"/>
    </hbox>
  </vbox>

  <!-- Canfield -->
  <vbox id="Canfield" name="&canfield.name;" menukey="&canfield.menukey;"
      hidden="true">
    <spacer class="row-spacer"/>
    <hbox>
      <spacer flex="1"/>
      <stack id="canfield-stock" class="card-pile"/><spacer flex="1"/>
      <stack id="canfield-waste" class="card-pile"/><spacer flex="2"/>
      <stack id="canfield-foundation-0" class="card-pile"/><spacer flex="1"/>
      <stack id="canfield-foundation-1" class="card-pile"/><spacer flex="1"/>
      <stack id="canfield-foundation-2" class="card-pile"/><spacer flex="1"/>
      <stack id="canfield-foundation-3" class="card-pile"/><spacer flex="2"/>
      <stack id="canfield-reserve" class="card-pile"/><spacer flex="1"/>
    </hbox>
    <spacer class="row-spacer"/>
    <hbox>
      <spacer flex="1"/>
      <spacer class="card-pile-spacer"/><spacer flex="1"/>
      <spacer class="card-pile-spacer"/><spacer flex="2"/>
      <stack id="canfield-pile-0" class="card-fan-down"/><spacer flex="1"/>
      <stack id="canfield-pile-1" class="card-fan-down"/><spacer flex="1"/>
      <stack id="canfield-pile-2" class="card-fan-down"/><spacer flex="1"/>
      <stack id="canfield-pile-3" class="card-fan-down"/><spacer flex="2"/>
      <spacer class="card-pile-spacer"/><spacer flex="1"/>
    </hbox>
  </vbox>

  <!-- Double Solitaire -->
  <vbox id="DoubleSol" name="&doublesol.name;" menukey="&doublesol.menukey;"
      hidden="true">
    <spacer class="row-spacer"/>
    <hbox>
      <spacer flex="1"/>
      <stack id="doublesol-stock" class="card-pile"/><spacer flex="1"/>
      <stack id="doublesol-waste" class="card-pile"/><spacer flex="1"/>
      <spacer class="card-pile-spacer"/><spacer flex="1"/>
      <stack id="doublesol-foundation-0" class="card-pile"/><spacer flex="1"/>
      <stack id="doublesol-foundation-1" class="card-pile"/><spacer flex="1"/>
      <stack id="doublesol-foundation-2" class="card-pile"/><spacer flex="1"/>
      <stack id="doublesol-foundation-3" class="card-pile"/><spacer flex="1"/>
    </hbox>
    <spacer class="row-spacer"/>
    <hbox>
      <spacer flex="1"/>
      <stack id="doublesol-pile-0" class="card-fan-down"/><spacer flex="1"/>
      <stack id="doublesol-pile-1" class="card-fan-down"/><spacer flex="1"/>
      <stack id="doublesol-pile-2" class="card-fan-down"/><spacer flex="1"/>
      <stack id="doublesol-pile-3" class="card-fan-down"/><spacer flex="1"/>
      <stack id="doublesol-pile-4" class="card-fan-down"/><spacer flex="1"/>
      <stack id="doublesol-pile-5" class="card-fan-down"/><spacer flex="1"/>
      <stack id="doublesol-pile-6" class="card-fan-down"/><spacer flex="1"/>
      <stack id="doublesol-pile-7" class="card-fan-down"/><spacer flex="1"/>
      <stack id="doublesol-pile-8" class="card-fan-down"/><spacer flex="1"/>
      <stack id="doublesol-pile-9" class="card-fan-down"/><spacer flex="1"/>
    </hbox>
  </vbox>

  <!-- FreeCell -->
  <vbox id="FreeCell" name="&freecell.name;" menukey="&freecell.menukey;"
      hidden="true">
    <spacer class="row-spacer"/>
    <hbox>
      <spacer flex="1"/>
      <stack id="freecell-cell-0" class="card-pile"/><spacer flex="1"/>
      <stack id="freecell-cell-1" class="card-pile"/><spacer flex="1"/>
      <stack id="freecell-cell-2" class="card-pile"/><spacer flex="1"/>
      <stack id="freecell-cell-3" class="card-pile"/><spacer flex="3"/>
      <stack id="freecell-foundation-0" class="card-pile"/><spacer flex="1"/>
      <stack id="freecell-foundation-1" class="card-pile"/><spacer flex="1"/>
      <stack id="freecell-foundation-2" class="card-pile"/><spacer flex="1"/>
      <stack id="freecell-foundation-3" class="card-pile"/><spacer flex="1"/>
    </hbox>
    <spacer class="row-spacer"/>
    <hbox>
      <spacer flex="2"/>
      <stack id="freecell-pile-0" class="card-fan-down"/><spacer flex="1"/>
      <stack id="freecell-pile-1" class="card-fan-down"/><spacer flex="1"/>
      <stack id="freecell-pile-2" class="card-fan-down"/><spacer flex="1"/>
      <stack id="freecell-pile-3" class="card-fan-down"/><spacer flex="1"/>
      <stack id="freecell-pile-4" class="card-fan-down"/><spacer flex="1"/>
      <stack id="freecell-pile-5" class="card-fan-down"/><spacer flex="1"/>
      <stack id="freecell-pile-6" class="card-fan-down"/><spacer flex="1"/>
      <stack id="freecell-pile-7" class="card-fan-down"/><spacer flex="2"/>
    </hbox>
  </vbox>

  <!-- Golf -->
  <!-- flex allow bottom alignment of golf-stack-row -->
  <vbox id="Golf" name="&golf.name;" menukey="&golf.menukey;"
      hidden="true" flex="1" difficultyLevels="&difficulty:golf;">
    <spacer class="row-spacer"/>
    <hbox id="golf-tableau-row">
      <spacer flex="1"/>
      <stack id="golf-pile-0" class="card-fan-down"/><spacer flex="1"/>
      <stack id="golf-pile-1" class="card-fan-down"/><spacer flex="1"/>
      <stack id="golf-pile-2" class="card-fan-down"/><spacer flex="1"/>
      <stack id="golf-pile-3" class="card-fan-down"/><spacer flex="1"/>
      <stack id="golf-pile-4" class="card-fan-down"/><spacer flex="1"/>
      <stack id="golf-pile-5" class="card-fan-down"/><spacer flex="1"/>
      <stack id="golf-pile-6" class="card-fan-down"/><spacer flex="1"/>
    </hbox>
    <spacer flex="1"/>
    <hbox id="golf-stock-row">
      <spacer flex="1"/>
      <stack id="golf-stock" class="card-slide"/>
      <spacer flex="2"/>
      <stack id="golf-foundation"  class="card-pile"/>
      <spacer flex="1"/>
    </hbox>
    <spacer class="row-spacer"/>
  </vbox>

  <!-- Grandfather Solitaire -->
  <vbox id="Grandfather" name="&grandfather.name;" menukey="&grandfather.menukey;"
      hidden="true">
    <spacer class="row-spacer"/>
    <hbox>
      <spacer flex="1"/>
      <stack id="grandfather-pile-0" class="card-fan-down"/><spacer flex="1"/>
      <stack id="grandfather-pile-1" class="card-fan-down"/><spacer flex="1"/>
      <stack id="grandfather-pile-2" class="card-fan-down"/><spacer flex="1"/>
      <stack id="grandfather-pile-3" class="card-fan-down"/><spacer flex="1"/>
      <stack id="grandfather-pile-4" class="card-fan-down"/><spacer flex="1"/>
      <stack id="grandfather-pile-5" class="card-fan-down"/><spacer flex="1"/>
      <stack id="grandfather-pile-6" class="card-fan-down"/><spacer flex="1"/>
      <vbox>
        <stack id="grandfather-foundation-0" class="card-pile"/><spacer class="row-spacer"/>
        <stack id="grandfather-foundation-1" class="card-pile"/><spacer class="row-spacer"/>
        <stack id="grandfather-foundation-2" class="card-pile"/><spacer class="row-spacer"/>
        <stack id="grandfather-foundation-3" class="card-pile"/><spacer flex="1"/>
      </vbox>
      <spacer flex="1"/>
    </hbox>
  </vbox>

  <!-- Gypsy -->
  <vbox id="Gypsy" name="&gypsy.name;" menukey="&gypsy.menukey;"
      hidden="true" difficultyLevels="&difficulty:easy-and-hard;">
    <spacer class="row-spacer"/>
    <hbox>
      <spacer flex="1"/>
      <stack id="gypsy-pile-0" class="card-fan-down"/><spacer flex="1"/>
      <stack id="gypsy-pile-1" class="card-fan-down"/><spacer flex="1"/>
      <stack id="gypsy-pile-2" class="card-fan-down"/><spacer flex="1"/>
      <stack id="gypsy-pile-3" class="card-fan-down"/><spacer flex="1"/>
      <stack id="gypsy-pile-4" class="card-fan-down"/><spacer flex="1"/>
      <stack id="gypsy-pile-5" class="card-fan-down"/><spacer flex="1"/>
      <stack id="gypsy-pile-6" class="card-fan-down"/><spacer flex="1"/>
      <stack id="gypsy-pile-7" class="card-fan-down"/><spacer flex="1"/>
      <vbox>
        <hbox>
          <stack id="gypsy-foundation-0" class="card-pile"/><spacer class="col-spacer"/>
          <stack id="gypsy-foundation-1" class="card-pile"/>
        </hbox><spacer class="row-spacer"/>
        <hbox>
          <stack id="gypsy-foundation-2" class="card-pile"/><spacer class="col-spacer"/>
          <stack id="gypsy-foundation-3" class="card-pile"/>
        </hbox><spacer class="row-spacer"/>
        <hbox>
          <stack id="gypsy-foundation-4" class="card-pile"/><spacer class="col-spacer"/>
          <stack id="gypsy-foundation-5" class="card-pile"/>
        </hbox><spacer class="row-spacer"/>
        <hbox>
          <stack id="gypsy-foundation-6" class="card-pile"/><spacer class="col-spacer"/>
          <stack id="gypsy-foundation-7" class="card-pile"/>
        </hbox><spacer class="row-spacer"/>
        <hbox>
          <spacer flex="1"/>
          <stack id="gypsy-stock" class="card-pile"/>
          <spacer flex="1"/>
        </hbox>
        <spacer flex="1"/>
      </vbox>
      <spacer flex="1"/>
    </hbox>
  </vbox>

  <!-- Klondike -->
  <vbox id="Klondike" name="&klondike.name;" menukey="&klondike.menukey;"
      hidden="true">
    <spacer class="row-spacer"/>
    <hbox>
      <spacer flex="1"/>
      <stack id="klondike-stock" class="card-pile"/><spacer flex="1"/>
      <stack id="klondike-waste" class="card-pile"/><spacer flex="1"/>
      <spacer class="card-pile-spacer"/><spacer flex="1"/>
      <stack id="klondike-foundation-0" class="card-pile"/><spacer flex="1"/>
      <stack id="klondike-foundation-1" class="card-pile"/><spacer flex="1"/>
      <stack id="klondike-foundation-2" class="card-pile"/><spacer flex="1"/>
      <stack id="klondike-foundation-3" class="card-pile"/><spacer flex="1"/>
    </hbox>
    <spacer class="row-spacer"/>
    <hbox>
      <spacer flex="1"/>
      <stack id="klondike-pile-0" class="card-fan-down"/><spacer flex="1"/>
      <stack id="klondike-pile-1" class="card-fan-down"/><spacer flex="1"/>
      <stack id="klondike-pile-2" class="card-fan-down"/><spacer flex="1"/>
      <stack id="klondike-pile-3" class="card-fan-down"/><spacer flex="1"/>
      <stack id="klondike-pile-4" class="card-fan-down"/><spacer flex="1"/>
      <stack id="klondike-pile-5" class="card-fan-down"/><spacer flex="1"/>
      <stack id="klondike-pile-6" class="card-fan-down"/><spacer flex="1"/>
    </hbox>
  </vbox>

  <!-- Mod 3 -->
  <vbox id="Mod3" name="&mod3.name;" menukey="&mod3.menukey;"
      hidden="true">
    <spacer class="row-spacer"/>
    <hbox id="mod3-row1">
      <spacer flex="1"/>
      <stack id="mod3-foundation-0" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-1" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-2" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-3" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-4" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-5" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-6" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-7" class="card-slide"/><spacer flex="1"/>
      <stack class="card-pile-spacer"/><spacer flex="1"/>
    </hbox><spacer class="row-spacer"/>
    <hbox id="mod3-row2">
      <spacer flex="1"/>
      <stack id="mod3-foundation-8" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-9" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-10" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-11" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-12" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-13" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-14" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-15" class="card-slide"/><spacer flex="1"/>
