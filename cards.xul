<?xml version="1.0"?>

<?xml-stylesheet href="chrome://cards/content/skin/cards.css" type="text/css"?>

<!DOCTYPE window [
<!ENTITY % cardsDTD SYSTEM "chrome://cards/locale/cards.dtd">
%cardsDTD;
]>

<window xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
  title  ="&cards-window-title;"
  id     ="cards-window"
  persist="screenX screenY width height sizemode"

  gameWonTitle="&game-won:title;"
  gameWonMessage="&game-won:message;"
  >
  <!-- gameWon attrs read and passed to nsIPromptService call.  Should be a stringbundle really -->

<!-- sets some var's needed for positioning of cards in javascript -->
<script type="application/x-javascript" src="skin/skin.js"/>

<script type="application/x-javascript" src="cardslib.js"/>
<script type="application/x-javascript" src="cardgame.js"/>
<script type="application/x-javascript" src="freecellmover.js"/>


<script type="application/x-javascript" src="acesup.js"/>
<script type="application/x-javascript" src="canfield.js"/>
<script type="application/x-javascript" src="doublesol.js"/>
<script type="application/x-javascript" src="freecell.js"/>
<script type="application/x-javascript" src="grandfather.js"/>
<script type="application/x-javascript" src="gypsy.js"/>
<script type="application/x-javascript" src="klondike.js"/>
<script type="application/x-javascript" src="regiment.js"/>
<script type="application/x-javascript" src="simon.js"/>
<script type="application/x-javascript" src="spider.js"/>
<script type="application/x-javascript" src="mod3.js"/>
<script type="application/x-javascript" src="yukon.js"/>

<commandset id="cards-commands">
  <command id="cmd:newgame" oncommand="Game.newGame();"/>
  <command id="cmd:hint"    oncommand="Game.hint();"/>
  <command id="cmd:undo"    oncommand="Game.undo();"/>
  <command id="cmd:restart" oncommand="Game.restart();"/>
  <command id="cmd:options" oncommand="Cards.showOptionsDialog();"/>
  <command id="cmd:setdifficulty" oncommand="Game.setDifficultyLevel(event.target.value);"/>
</commandset>

<keyset id="cards-keys">
  <key id="key:newgame" key="&key:newgame;" modifiers="accel" observes="command:newgame"/>
  <key id="key:restart" key="&key:restart;" modifiers="accel" observes="command:restart"/>
  <key id="key:undo"    key="&key:undo;"    modifiers="accel" observes="command:undo"/>
  <key id="key:hint"    key="&key:hint;"    modifiers="accel" observes="command:hint"/>
</keyset>


<!-- display -->
<toolbar id="cards-toolbar">
  <toolbarbutton id="game-type-menu" type="menu" label="&gametype-menu:label;">
    <menupopup id="menupopup-gametypes" oncommand="Cards.playGame(event.target.value);">
      <menuitem label="&acesup-name;"      accesskey="&mi-acesup:accesskey;"      value="AcesUp"     />
      <menuitem label="&canfield-name;"    accesskey="&mi-canfield:accesskey;"    value="Canfield"   />
      <menuitem label="&doublesol-name;"   accesskey="&mi-doublesol:accesskey;"   value="DoubleSol"  />
      <menuitem label="&freecell-name;"    accesskey="&mi-freecell:accesskey;"    value="FreeCell"   />
      <menuitem label="&grandfather-name;" accesskey="&mi-grandfather:accesskey;" value="Grandfather"/>
      <menuitem label="&gypsy-name;"       accesskey="&mi-gypsy:accesskey;"       value="Gypsy"      />
      <menuitem label="&klondike-name;"    accesskey="&mi-klondike:accesskey;"    value="Klondike"   />
      <menuitem label="&mod3-name;"        accesskey="&mi-mod3:accesskey;"        value="Mod3"       />
      <menuitem label="&regiment-name;"    accesskey="&mi-regiment:accesskey;"    value="Regiment"   />
      <menuitem label="&simon-name;"       accesskey="&mi-simon:accesskey;"       value="SimpleSimon"/>
      <menuitem label="&spider-name;"      accesskey="&mi-spider:accesskey;"      value="Spider"     />
      <menuitem label="&yukon-name;"       accesskey="&mi-yukon:accesskey;"       value="Yukon"      />
    </menupopup>
  </toolbarbutton>
  <!-- difficulty level menu is show/hidden by removing/adding style="display:none" -->
  <toolbarbutton id="game-difficulty-menu" type="menu" label="&m-game-difficulty:label;"
    tooltiptext="&m-game-difficulty:tooltip;" command="cmd:setdifficulty">
    <menupopup>
      <menuitem label="&difficulty-easy:label;"   value="easy"/>
      <menuitem label="&difficulty-medium:label;" value="medium"/>
      <menuitem label="&difficulty-hard:label;"   value="hard"/>
    </menupopup>
  </toolbarbutton>
  <!-- toolbarbutton command="command:options" label="&options:label;" tooltiptext="&options:tooltip;"/ -->
  <toolbarbutton command="cmd:newgame" label="&newgame:label;" tooltiptext="&newgame:tooltip;"/>
  <toolbarbutton command="cmd:restart" label="&restart:label;" tooltiptext="&restart:tooltip;"/>
  <toolbarbutton command="cmd:undo"    label="&undo:label;"    tooltiptext="&undo:tooltip;"/>
  <toolbarbutton command="cmd:hint"    label="&hint:label;"    tooltiptext="&hint:tooltip;"/>
  <spacer flex="1"/>
  <hbox id="score-panel">
    <description value="&statusbar-score-label;"/>
    <description id="score-display"/>
  </hbox>
</toolbar>

<!--
spacing at top and between rows of cards is done with <spacer class="row-spacer"/>s
rather than CSS, because margins and padding are not included in the result of
boxEl.boxObject.top/left etc, so there is a jump (of the num of px of padding)
when starting to move a card, which looks abysmal

margin on the <stack/> lets you drag cards out of the edge of the window without
forcing the stack to expand, which pulls the whole content of the game right, or
causes slowdowns (dragging off bottom). padding stops the content from actually
using those 100px :)  thanks Neil.
-->

<stack id="current-game-display" style="margin: 0 -100px -1000px 0; padding: 0 100px 0 0;">
  <!-- Aces Up -->
  <vbox id="AcesUp" style="display:none" name="&acesup-name;">
    <spacer class="row-spacer"/>
    <hbox>
