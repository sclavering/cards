      <stack id="mod3-foundation-20" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-21" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-22" class="card-slide"/><spacer flex="1"/>
      <stack id="mod3-foundation-23" class="card-slide"/><spacer flex="1"/>
